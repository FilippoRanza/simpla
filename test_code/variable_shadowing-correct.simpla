a, b: real;

func fibonacci(n: integer) : integer
    a, b, i, tmp: integer;
body
    a = 0;
    b = 1;
    for i = 0 to n do
        tmp = a + b;
        a = b;
        b = tmp;
    end;
    return a;
end;

body
    a = 5.6;
